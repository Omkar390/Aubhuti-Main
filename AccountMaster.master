        function ChkDupEmail() {
            var retval = false;
            $.ajax({
                type: "POST",
                url: "WebMethods.aspx/CheckDupEmail",
                data: '{ email: "' + document.getElementById("ctl00_ctl00_ContentPlaceHolder1_pCtEmail").value +
                          '",t_contact_id: "' + document.getElementById("ctl00_ctl00_ContentPlaceHolder1_hdncontactId").value +
                          '"}',
                contentType: "application/json; charset=utf-8",
                async: false,
                dataType: "json",
                success: function (data) {
                    if (data.d == 0) {
                        retval = true;
                        //return SaveContact();
                    }
                    else if (data.d == 1) {
                        alert("Duplicate email.");
                        retval = false;
                    }
                    else if (data.d == 2) {
                        $("#AllowAlert").show();
                        retval = false;
                    }
                    //alert(data.d);
                },
                failure: function(response) {
                    alert(response.d);
                }
            });
            return retval;
        }

