                        '",fax: "' + $("#ctl00_ctl00_ContentPlaceHolder1_pFax").intlTelInput("getNumber", intlTelInputUtils.numberFormat.E164) +
                        '",website: "' + document.getElementById("ctl00_ctl00_ContentPlaceHolder1_pWebsite").value +
                        '",sic: "' + document.getElementById("ctl00_ctl00_ContentPlaceHolder1_pIndustry").value +
                        '",industry: "' + document.getElementById("ctl00_ctl00_ContentPlaceHolder1_pIndustry").value +
                        '",annualrevenue: "' + document.getElementById("ctl00_ctl00_ContentPlaceHolder1_pAnnRev").value +
                        '",actualinvoice: "' + document.getElementById("ctl00_ctl00_ContentPlaceHolder1_pInvoices").value +
                        '",numberofemployees: "' + document.getElementById("ctl00_ctl00_ContentPlaceHolder1_pEmployees").value +
                        '",ownership: "' + selectedOwnership +
