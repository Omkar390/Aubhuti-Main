    function SaveAccountAttr()   {

        var AttrType = document.getElementById("ctl00_ctl00_ContentPlaceHolder1_litAttrType").innerHTML;
        if (AttrType == 'System' || AttrType == 'Profit Recovery Firm') {
            var AttrDesc = document.getElementById("ctl00_ctl00_ContentPlaceHolder1_txtDescription").value;
        }
        else {
            var AttrDesc = document.getElementById("ctl00_ctl00_ContentPlaceHolder1_txtDesc1").value;
        }
        validated = true;

        $.ajax({
            type: "POST",
            url: "WebMethods.aspx/ValidateDupAttr",
            data: '{    AttrType: "' + AttrType +
                    '", AttrDesc: "' + AttrDesc  + 
                  '",AccountId: "' + document.getElementById("ctl00_ctl00_ContentPlaceHolder1_hdnaccountId").value + '"}',
            contentType: "application/json; charset=utf-8",
            async: false,
            dataType: "json",
            success: function(data) {
            if(data.d == false){
                alert('Attribute already added.');
                validated = false;
                return false;
                }
            },
            failure: function(response) {
                alert(response.d);
            }
        });

        if (!validated)
            return false; 
        else
        {
            validated = true;       
            
            $('#showwait').click();
            $.ajax({
                type: "POST",
                url: "WebMethods.aspx/SaveAccountAttr",
                data: '{ CoId: "' + document.getElementById("ctl00_ctl00_ContentPlaceHolder1_hdnaccountId").value +
                      '",AttrType: "' + AttrType +
                      '",TypeId: "' + document.getElementById("ctl00_ctl00_ContentPlaceHolder1_hdnValue").value +
                      '",Description: "' + AttrDesc + '"}',
                contentType: "application/json; charset=utf-8",
                async: false,
                dataType: "json",
                success: function(data) {
                    if (data.d != 0 ){
                        $("#addAcctAttr").hide();
                        eval(<%=BindAccount %>);
                    }
                },
                failure: function(response) {
                    alert("Failure");
                    alert(data.d);
                    alert(response.d);
                }
                }); 
            }
            return false;
        }

    function SaveInvent()   {

        $('#showwait').click();
        var InvSel = document.getElementById("ctl00_ctl00_ContentPlaceHolder1_pddlInv");
        var selectedInv = InvSel.options[InvSel.selectedIndex].value;

        $.ajax({
            type: "POST",
            url: "WebMethods.aspx/SaveInvent",
            data: '{ CoId: "' + document.getElementById("ctl00_ctl00_ContentPlaceHolder1_hdnaccountId").value +
                    '",CtId: "' + document.getElementById("ctl00_ctl00_ContentPlaceHolder1_hdncontactId").value +
                    '",InvId: "' + selectedInv + '"}',
            contentType: "application/json; charset=utf-8",
            async: false,
            dataType: "json",
            success: function(data) {
                if (data.d != 0 ){
                    $("#addInvent").hide();
                    eval(<%=BindContact %>);
                }
            },
            failure: function(response) {
                alert("Failure");
                alert(data.d);
                alert(response.d);
            }
            }); 
        }

        function SaveOwner() {
            var OwnerSel = document.getElementById("ctl00_ctl00_ContentPlaceHolder1_ddlOwner");
            var selectedOwner = OwnerSel.options[OwnerSel.selectedIndex].value;

            $.ajax({
                type: "POST",
                url: "WebMethods.aspx/SaveOwner",
                data: '{ t_account_id: "' + document.getElementById("ctl00_ctl00_ContentPlaceHolder1_hdnaccountId").value +
                      '",owner: "' + selectedOwner  + '"}',
                contentType: "application/json; charset=utf-8",
                async: false,
                dataType: "json",
                success: function(data) {
                    $("#changeOwner").hide();
                    eval(<%=BindAccount %>);
                },
                failure: function(response) {
                    alert(response.d);
                }
                }); 
            }

    </script>

 <script type="text/javascript">
        function newemail() {
            document.getElementById("ctl00_ctl00_ContentPlaceHolder1_pSendto").value = document.getElementById("ctl00_ctl00_ContentPlaceHolder1_pCtEmail").value;
            $("#newemail").show();
        }

        function settemplate(ddl, val) {

            if (document.getElementById("ctl00_ctl00_ContentPlaceHolder1_pSendto").value.trim() == '') {
                alert("To box required, in order to select a template.");
                document.getElementById("ctl00_ctl00_ContentPlaceHolder1_ddlSubject").value = '';
                return false;
            }
            //var strBody = escape(document.getElementById("ctl00_ctl00_ContentPlaceHolder1_pEmailBody").value);
            $.ajax({
