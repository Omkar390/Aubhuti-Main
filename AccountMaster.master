                        '",tier: "' + selectedTier +
                        '",invoicerange: "' + selectedInvRange +
                        '",valdate: "' + document.getElementById("ctl00_ctl00_ContentPlaceHolder1_pdtVal").value  +
                        '",ownerid: "' + selectedOwner  +
                        '",microregion: "' + selectedMicroRegion  +
                        '",accountstatus: "' + selectedStatus + '"}',
                contentType: "application/json; charset=utf-8",
                async: false,
                dataType: "json",
                success: function(data) {                    
						$("#editco").hide();
							document.getElementById("ctl00_ctl00_ContentPlaceHolder1_hdnaccountId").value = data.d ;
							document.getElementById("ctl00_ctl00_ContentPlaceHolder1_shdrCo").innerText = document.getElementById("ctl00_ctl00_ContentPlaceHolder1_pCoName").value;
