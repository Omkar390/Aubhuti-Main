                          '",t_account_id: "' + document.getElementById("ctl00_ctl00_ContentPlaceHolder1_hdnaccountId").value +
                          '"}',
                    contentType: "application/json; charset=utf-8",
                    async: false,
                    dataType: "json",
                    success: function (data) {
                        if (data.d == 1) {
                            $("#AllowDupAcct").show();
                            dupchk = 0;
                        }
                        else {
                            dupchk = 1;
                        }
                        //alert(data.d);
                    },
                    failure: function (response) {
                        alert(response.d);
                    }
                });
                if (dupchk == 0) {
                    return false;
                }
            }

