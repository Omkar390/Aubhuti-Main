            $.ajax({
                type: "POST",
                url: "WebMethods.aspx/SaveAccount",
                data: '{ t_account_id: "' + document.getElementById("ctl00_ctl00_ContentPlaceHolder1_hdnaccountId").value +
                        '",name: "' + document.getElementById("ctl00_ctl00_ContentPlaceHolder1_pCoName").value +
