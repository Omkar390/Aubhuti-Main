            var CountrySel = document.getElementById("ctl00_ctl00_ContentPlaceHolder1_pCountry");
            var selectedCountry = CountrySel.options[CountrySel.selectedIndex].value;

            var OwnershipSel = document.getElementById("ctl00_ctl00_ContentPlaceHolder1_pOwnership");
            var selectedOwnership = OwnershipSel.options[OwnershipSel.selectedIndex].value;

